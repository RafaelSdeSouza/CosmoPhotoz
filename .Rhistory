library(CosmoPhotoz)
require(ggplot2)
counts <- c(18,17,15,20,10,20,25,13,12)
outcome <- gl(3,1,9)
treatment <- gl(3,3)
glm.D <- glm(counts ~ outcome + treatment, family=poisson())
ggplot.glm(glm.D)
